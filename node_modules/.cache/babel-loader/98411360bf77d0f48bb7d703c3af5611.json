{"ast":null,"code":"export default {\n  data() {\n    return {\n      show: true,\n      name: '',\n      phone: '',\n      email: '',\n      password: ''\n    };\n  },\n\n  methods: {\n    openIn() {\n      this.show = false;\n    },\n\n    openReg() {\n      this.show = true;\n    },\n\n    async register(url) {\n      const postData = {\n        name: this.name,\n        phone: this.number,\n        email: this.email,\n        password: this.password\n      };\n      const rec = {\n        method: \"POST\",\n        body: JSON.stringify(postData)\n      };\n      await fetch(url, rec);\n      console.log(postData);\n    },\n\n    async log(url) {\n      const postData = {\n        email: this.email,\n        password: this.password\n      };\n      await fetch(url, {\n        method: \"POST\",\n        body: JSON.stringify(postData)\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkEA,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,IADD;AAELC,UAAI,EAAC,EAFA;AAGLC,WAAK,EAAE,EAHF;AAILC,WAAK,EAAE,EAJF;AAKLC,cAAQ,EAAE;AALL,KAAP;AAQD,GAVY;;AAYbC,SAAO,EAAE;AAEPC,UAAM,GAAG;AACP,WAAKN,IAAL,GAAY,KAAZ;AACD,KAJM;;AAMPO,WAAO,GAAG;AACR,WAAKP,IAAL,GAAY,IAAZ;AACD,KARM;;AAUP,UAAMQ,QAAN,CAAeC,GAAf,EAAoB;AAClB,YAAMC,QAAO,GAAI;AACfT,YAAI,EAAE,KAAKA,IADI;AAEfC,aAAK,EAAE,KAAKS,MAFG;AAGfR,aAAK,EAAE,KAAKA,KAHG;AAIfC,gBAAQ,EAAE,KAAKA;AAJA,OAAjB;AAOA,YAAMQ,GAAE,GAAI;AACVC,cAAM,EAAE,MADE;AAEVC,YAAI,EAAEC,IAAI,CAACC,SAAL,CAAeN,QAAf;AAFI,OAAZ;AAMA,YAAMO,KAAK,CAACR,GAAD,EAAMG,GAAN,CAAX;AAEAM,aAAO,CAACC,GAAR,CAAYT,QAAZ;AACD,KA3BM;;AA6BP,UAAMS,GAAN,CAAUV,GAAV,EAAe;AACb,YAAMC,QAAO,GAAI;AACfP,aAAK,EAAE,KAAKA,KADG;AAEfC,gBAAQ,EAAE,KAAKA;AAFA,OAAjB;AAKA,YAAMa,KAAK,CAACR,GAAD,EAAM;AACfI,cAAM,EAAE,MADO;AAEfC,YAAI,EAAEC,IAAI,CAACC,SAAL,CAAeN,QAAf;AAFS,OAAN,CAAX;AAIF;;AAvCO;AAZI,CAAf","names":["data","show","name","phone","email","password","methods","openIn","openReg","register","url","postData","number","rec","method","body","JSON","stringify","fetch","console","log"],"sourceRoot":"","sources":["/Users/egoravdeev/Documents/odgeto_test/src/components/RegForm.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"content\" v-if=\"show\">\n      <div class=\"reg_content\">\n        <h1 class=\"reg_title\">Регистрация начинается отсюда</h1>\n        <div class=\"reg_des\">Пожалуйста указывайте свои настоящие данные, они будут использоваться в дальнейшем на собеседовании.</div>\n        <div class=\"reg\">\n          <div class=\"reg_form\">\n            <form @submit.prevent=\"register\" class=\"reg_form-content\">\n              <label class=\"reg_label\" for=\"name\">Ваше ФИО</label>\n              <input v-model=\"name\"  class=\"reg_input\" placeholder=\"Name\" name=\"name\" type=\"text\">\n\n              <label class=\"reg_label\" for=\"email\">Электронная почта</label>\n              <input v-model=\"email\" class=\"reg_input\" placeholder=\"email@gmail.com\" name=\"email\" type=\"email\">\n\n              <label class=\"reg_label\" for=\"phone\">Ваш номер телефона</label>\n              <input v-model.number=\"phone\" class=\"reg_input\" placeholder=\"88005555555\" name=\"phone\" type=\"number\">              \n              \n              <label class=\"reg_label\" for=\"pass\">Пароль</label>\n              <input v-model=\"password\" class=\"reg_input\" placeholder=\"password\" name=\"pass\" type=\"password\">\n\n              <div class=\"reg_conf\">\n                <input class=\"reg_check\" type=\"checkbox\">\n                <span class=\"reg_conf-des\">Я согласен с <a href=\"#\">политикой конфиденциальности</a></span>\n              </div>\n\n              <button type=\"submit\" class=\"reg_btn\" @click=\"register\">Создать аккаунт</button>\n            </form>\n\n            <div class=\"reg_log\">Уже есть аккаунт? <button class=\"reg_log-btn\" @click=\"openIn\" >Войдите</button> в него.</div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"info\"></div>\n    </div>\n\n    <div class=\"content\" v-else >\n      <div class=\"reg_content\">\n        <h1 class=\"reg_title\">Авторизация</h1>\n        <div class=\"reg\">\n          <div class=\"reg_form\">\n            <form action=\"\" class=\"reg_form-content\">\n\n\n              <label class=\"reg_label\" for=\"email\">Введите вашу почту или телефон</label>\n              <input class=\"reg_input\" placeholder=\"email@gmail.com\" name=\"email\" type=\"text\">             \n              \n              <label class=\"reg_label\" for=\"pass\">Пароль</label>\n              <input class=\"reg_input\" placeholder=\"password\" name=\"pass\" type=\"password\">\n\n              <button class=\"reg_btn\" @click=\"log\">Войти</button>\n            </form>\n\n            <div class=\"reg_log\">Еще нет аккаунта? <button class=\"reg_log-btn\" @click=\"openReg\">Зарегестрируйтесь.</button></div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"info\"></div>\n    </div>\n  </div>\n</template> \n\n\n<script>\nexport default {\n  data() {\n    return {\n      show: true,\n      name:'',\n      phone: '',\n      email: '',\n      password: '',\n\n    }\n  },\n\n  methods: {\n\n    openIn() {\n      this.show = false;\n    },\n\n    openReg() {\n      this.show = true;\n    },\n\n    async register(url) {\n      const postData = {\n        name: this.name,\n        phone: this.number,\n        email: this.email,\n        password: this.password\n      };\n    \n      const rec = {\n        method: \"POST\",\n        body: JSON.stringify(postData)\n      };\n\n\n      await fetch(url, rec);\n\n      console.log(postData);\n    },\n\n    async log(url) {\n      const postData = {\n        email: this.email,\n        password: this.password\n      };\n\n      await fetch(url, {\n        method: \"POST\",\n        body: JSON.stringify(postData),\n      });\n    }\n  }\n}\n</script>\n\n\n\n// ----------------------------\n<style lang=\"scss\">\n  * {\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n    font-family: 'Montserrat', sans-serif; \n  }\n\n  body {\n    background: #000;\n  }\n\n  .content {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n\n  .reg {\n    &_content {\n      padding: 100px 0;\n      max-width: 576px;\n    }\n    &_form {\n      margin-top: 50px;\n      &-content {\n        display: flex;\n        flex-direction: column;\n      }\n    }\n    &_title {\n      font-weight: 600;\n      font-size: 32px;\n      color: white;\n      text-align: center;\n      margin: 0 auto;\n    }\n    &_des {\n      font-weight: 400;\n      font-size: 18px;\n      text-align: center;\n      color: #F6F6F6;\n      margin: 0 auto;\n      margin-top: 15px;\n    }\n    &_input {\n      position: relative;\n      border: 1.5px solid #F6F6F6;\n      border-radius: 6px;\n      padding: 12px 8px 12px 24px;\n      background: none;\n      width: 575px;\n      height: 51px;\n      margin-top: 10px;\n      margin-bottom: 25px;\n      font-weight: 400;\n      font-size: 18px;\n      outline: none;\n      color: #F6F6F6;\n    }\n    &_label {\n      font-weight: 500;\n      font-size: 18px;\n      color: #F6F6F6;\n    }\n\n    &_conf {\n      &-des {\n        margin-left: 10px;\n        font-weight: 400;\n        font-size: 16px;\n        color: #F4F4F4;\n        a {\n          font-weight: 400;\n          font-size: 16px;\n          color: #F4F4F4;\n        }\n      }\n    }\n\n    &_btn {\n      background: #FFED00;\n      border: 1.5px solid #FFED00;\n      border-radius: 12px;\n      height: 50px;\n      font-weight: 600;\n      font-size: 18px;\n      text-align: center;\n      color: #000000;\n      margin-top: 48px;\n      cursor: pointer;\n    }\n    &_log {\n      font-weight: 400;\n      font-size: 16px;\n      color: #F6F6F6;\n      text-align: center;\n      margin-top: 10px;\n      &-btn {\n        font-weight: 400;\n        font-size: 16px;\n        color: #FFED00;\n        background: none;\n        border: none;\n        cursor: pointer;\n      }\n    }\n  }\n\n</style>\n"]},"metadata":{},"sourceType":"module"}